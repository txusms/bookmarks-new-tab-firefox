{
  "manifest_version": 2,
  "name": "Bookmarks new tab (simple)",
  "version": "0.0.1",
  "author": "Txus MS",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }, 
  "description":"A Fast and Light weight new tab (Fork from Reac new tab).",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  }
}
